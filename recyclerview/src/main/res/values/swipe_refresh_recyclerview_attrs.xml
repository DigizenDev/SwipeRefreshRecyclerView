<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AVLoadingIndicatorView">
        <attr name="indicator">
            <flag name="BallSpinFadeLoader" value="22"/>
        </attr>
        <attr name="indicator_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="SwipeRefreshRecyclerView">
        <attr name="Mode" format="enum">
            <enum name="BOTH" value="0"/>
            <enum name="REFRESH" value="1"/>
            <enum name="LOAD_MORE" value="2"/>
        </attr>
    </declare-styleable>
</resources>